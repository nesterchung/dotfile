#!/usr/bin/env zsh

vi-note(){
  topic=$1
  $EDITOR $NOTE_FILES/${topic}.md
}
export NOTE_FILES=$DROPBOX_HOME/markdown
#markdown alias
for topic in {android,ios,git,dp,kt}; do
  alias vi-note-${topic}="$EDITOR $NOTE_FILES/${topic}.md";
done

# utils for search google 
alias ggg='googler -n 7 -c ru -l ru'

